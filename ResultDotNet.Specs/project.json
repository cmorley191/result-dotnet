{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "Should.fs",
        "ResultSpec.fs",
        "ReadmeSpec.fs"
      ]
    }
  },
  "dependencies": {
    "System.Runtime.Serialization.Primitives": "4.1.1",
    "System.Data.Common": "4.3.0",
    "nunit": "3.4.0",
    "dotnet-test-nunit": "3.4.0-beta-3",
    "ResultDotNet": { 
      "target": "project"
    }
  },
  "testRunner": "nunit",
  "tools": {
    "dotnet-compile-fsc":"1.0.0-preview2-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        },
        "FSharp.Core": "4.0.1.5"
      },
      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  }
}
